#include<iostream>
#include<string>
using namespace std;

class Student
{
private:
    string name;
    float percentage;

public:
   
    Student()
    {
        name="Disha";
        percentage=96;
    }

    void display()
    {
        cout << "Student name: " << name << endl;
        cout << "Student percentage: " << percentage << endl;
    }
};

int main()
{
    Student s1;  
    s1.display();
    return 0;
}
