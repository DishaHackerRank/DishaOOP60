#include<iostream>
#include<string>
using namespace std;

class Student
{
private:
    string name;
    float percentage;

public:

    Student(string n, float p)
    {
        name = n;
        percentage = p;
    }

    
    Student(Student &s1)
    {
        name = s1.name;
        percentage = s1.percentage;
    }

    void display()
    {
        cout << "Student name: " << name << endl;
        cout << "Student percentage: " << percentage << endl;
    }
};

int main()
{
    Student s1("Disha", 96);  
    Student s2(s1);          

    s1.display();
    s2.display();

    return 0;
}
