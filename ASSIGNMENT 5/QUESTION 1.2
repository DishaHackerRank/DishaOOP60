#include <iostream>
using namespace std;

class Sum {
    int n, sum;

public:
  
    Sum(int num) {
        n = num;
        sum = 0;

        for (int i = 0; i < n; i++) {
            sum += i;
        }

        cout << "Sum (Parameterized) = " << sum << endl;
    }

    
    Sum( Sum &s1) {
        n = s1.n;
        sum = s1.sum;

        cout << "Sum (Copy Constructor) = " << sum << endl;
    }
};

